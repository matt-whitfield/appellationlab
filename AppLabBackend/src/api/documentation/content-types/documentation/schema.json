{
  "kind": "collectionType",
  "collectionName": "documentations",
  "info": {
    "singularName": "documentation",
    "pluralName": "documentations",
    "displayName": "Documentation"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "text"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "shared.image",
        "shared.markdown",
        "shared.callout"
      ]
    },
    "featured": {
      "type": "boolean"
    },
    "lastUpdated": {
      "type": "datetime"
    },
    "sortOrder": {
      "type": "integer"
    }
  }
}
